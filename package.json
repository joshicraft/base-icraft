{
  "name": "icraft",
  "version": "1.3.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run copy-admin-files",
    "lint": "vue-cli-service lint",
    "copy-admin-files": "copyfiles admin/**/*.html admin/**/*.yml ./dist",
    "start:lambda": "netlify-lambda serve func -c webpack.server.js",
    "build:lambda": "netlify-lambda build func",
    "build-node": "node build/build.js"
  },
  "devDependencies": {
    "@mdi/font": "^2.5.94",
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-service": "^3.0.5",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^2.0.0",
    "copyfiles": "^2.1.0",
    "dotenv": "^6.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "netlify-lambda": "^1.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "register-service-worker": "^1.0.0",
    "sharp": "^0.21.0",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.17",
    "vue-cli-plugin-externals": "^1.0.4",
    "vue-i18n": "^8.3.2",
    "vue-meta": "^1.5.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vue-waypoint": "^3.2.2",
    "vuetify": "^1.2.3",
    "vuetify-loader": "^1.0.6",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "vue-scroll": "^2.1.9"
  }
}

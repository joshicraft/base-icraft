{
  "name": "icraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run copy-admin-files",
    "lint": "vue-cli-service lint",
    "copy-admin-files": "copyfiles admin/**/*.html admin/**/*.yml ./dist",
    "start:lambda": "netlify-lambda serve func -c webpack.server.js",
    "build:lambda": "netlify-lambda build func",
    "dockerbuild": "rm -rf node_modules/sharp && docker run -v \"$PWD\":/var/task lambci/lambda:build-nodejs8.10 npm install"
  },
  "dependencies": {
    "@mdi/font": "^2.5.94",
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "copyfiles": "^2.1.0",
    "glob-fs": "^0.1.7",
    "gsap": "^2.0.2",
    "material-design-icons-iconfont": "^4.0.2",
    "register-service-worker": "^1.0.0",
    "sharp": "^0.21.0",
    "vue": "^2.5.17",
    "vue-analytics": "^5.8.0",
    "vue-i18n": "^7.4.0",
    "vue-meta": "^1.5.2",
    "vue-parallaxy": "^1.1.1",
    "vue-router": "^3.0.1",
    "vue-videobg": "^2.0.0",
    "vuetify": "^1.2.3",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "web-audio-player": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-service": "^3.0.5",
    "dotenv": "^6.1.0",
    "glob": "^7.1.3",
    "gm": "^1.23.1",
    "netlify-lambda": "^1.0.2",
    "spawn-sync": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  }
}

@media (max-height: 500px) and (orientation: landscape) and (max-width: 800px)
  .title-a
    max-width 80% !important

  .directions
    display: none

@media screen and (max-width: 700px)

  .jumbo-bot-arrow
    margin-top -(94/2)px
